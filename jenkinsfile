pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building.. This is the build phast'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing.. This is the testing phase'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....  This is the deployment phase. right'
            }
        }
	stage('Postdeploy') {
		steps {
			echo 'Postdeployment phase.right...'
		}
	}
    }
}
